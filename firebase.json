{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/timestamps",
        "destination": "nodejsapp"
      }
    ],
    "functions": {
      "predeploy": "npm --prefix functions run build"
    }
  }
  
}
